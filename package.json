{
  "name": "fuyou-wakarundesu",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "setup": "bash scripts/setup-local-env.sh",
    "dev": "next dev --turbopack",
    "predev": "node scripts/check-env.js --quick",
    "build": "next build",
    "start": "next start",
    "e2e:start": "npm run build && npx next start -p 3333",
    "lint": "next lint --max-warnings 50",
    "type-check": "tsc --noEmit",
    "verify-env": "node scripts/verify-env.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test --reporter=line",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:ci && npm run test:e2e",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --config=.lighthouserc.js",
    "lighthouse:desktop": "lhci collect --settings.emulatedFormFactor=desktop && lhci assert",
    "perf:audit": "npm run lighthouse && npm run test:e2e",
    "perf:report": "lhci collect --config=.lighthouserc.js --numberOfRuns=5"
  },
  "dependencies": {
    "@sentry/nextjs": "9.33.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "date-fns": "^4.1.0",
    "file-saver": "2.0.5",
    "i18next": "^25.3.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "json2csv": "6.0.0-alpha.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "next-pwa": "5.6.0",
    "nextjs-google-analytics": "2.3.7",
    "openai": "4.104.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.6.0",
    "swr": "^2.3.4",
    "workbox-window": "7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.0",
    "tailwindcss": "^4",
    "tslib": "^2.8.1",
    "typescript": "^5",
    "web-vitals": "^5.0.3",
    "zod": "^3.22.4"
  }
}
